m) {\r\n      if (item === \"用户中心\") {\r\n        this.$router.push(\"/message\");\r\n      } else {\r\n        this.$router.push(\"/order\");\r\n      }\r\n    },\r\n    goGoods(goods,index) {\r\n      this.$router.push({ path: \"/goods\", query: { goodsInfo: goods[index] } });\r\n    },\r\n    search() {\r\n      this.axios({\r\n        method: \"post\",\r\n        url: this.GLOBAL.contentUrl + \"/goods/search\",\r\n        data: {\r\n          content: this.content,\r\n          currentPage: 1,\r\n          pageSize: 10,\r\n        },\r\n      }).then((res) => {\r\n        console.log(res);\r\n        this.goods = res.data.data.Goods.content\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.input {\r\n  margin: auto;\r\n  border: 3px solid #26a69a;\r\n  border-radius: 20px;\r\n  width: 500px;\r\n  height: 50px;\r\n  display: flex;\r\n}\r\n.inputBorder {\r\n  width: 450px;\r\n  height: 45px;\r\n  margin-left: 20px;\r\n  border: 0; // 去除未选中状态边框\r\n  outline: none; // 去除选中状态边框\r\n  background-color: rgba(0, 0, 0, 0);\r\n}\r\n.user {\r\n  position: absolute;\r\n  right: 30px;\r\n}\r\n.image {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50px;\r\n  border: 3px solid #26a69a;\r\n}\r\n.search {\r\n  width: 50px;\r\n  height: 50px;\r\n  background-color: #26a69a;\r\n  border-radius: 0 20px 20px 0;\r\n  margin: -3px;\r\n}\r\n.icon {\r\n  width: 30px;\r\n  height: 30px;\r\n  margin: 10px;\r\n}\r\n</style>\r\n"]}]}